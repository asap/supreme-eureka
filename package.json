{
  "name": "supreme-eureka",
  "version": "1.0.0",
  "description": "Doctor/Patient Portal",
  "main": "index.js",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "nodemon": "^1.18.10",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "webpack": "4.28.3"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "6.6.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server:dev": "nodemon server/index.js",
    "server:prod": "node server/index.js",
    "client": "npm run start --prefix client",
    "build": "npm run build --prefix client",
    "install:client": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "install:server": "npm install",
    "start": "concurrently --kill-others-on-fail \"npm run server:prod\" \"npm run client\"",
    "dev:pre": "concurrently --kill-others-on-fail \"npm run server:dev\" \"npm run client\"",
    "dev": "concurrently --kill-others \"webpack --watch\" \"npm run server:dev\" \"npm run client\"",
    "db:seed": "node_modules/.bin/sequelize db:seed:undo:all && node_modules/.bin/sequelize db:seed:all",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "postinstall": "npm run install:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asap/supreme-eureka.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asap/supreme-eureka/issues"
  },
  "homepage": "https://github.com/asap/supreme-eureka#readme"
}
